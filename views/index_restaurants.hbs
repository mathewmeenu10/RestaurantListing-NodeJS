<h1>All Restaurants</h1>
<table border="1">
    <thead>
        <th>ID</th><th>Name</th><th>Cusine</th><th>Borough</th><th>Address</th>
    </thead>
    <tbody>
       
    {{#each restaurants}}
        <tr> 
            <td>{{restaurant_id}}</td>
            <td>{{name}}</td>
            <td>{{cusine}}</td>
            <td>{{borough}}</td>
            <td>{{address.building}}, {{address.street}}</td>
            
        </tr>
    {{/each}}
    </tbody>
</table>



